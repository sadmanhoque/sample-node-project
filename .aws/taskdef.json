{
    "executionRoleArn": "arn:aws:iam::516532666009:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "my-first-task",
            "image": "516532666009.dkr.ecr.ca-central-1.amazonaws.com/my-first-ecr-repo:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ],
            "memory": 512,
            "cpu": 256
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}